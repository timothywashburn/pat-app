{
  "expo": {
    "name": "pat-app",
    "slug": "pat-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dev.timothyw.patapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "light": "./assets/images/icon.png",
        "dark": "./assets/images/icon.png",
        "tinted": "./assets/images/icon.png"
      },
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "dev.timothyw.patapp",
      "associatedDomains": [
        "applinks:pat.timothyw.dev",
        "applinks:dev-timothyw-patapp.ngrok.io"
      ]
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F5F7FA"
      },
      "package": "dev.timothyw.patapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "pat.timothyw.dev",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "dev-timothyw-patapp.ngrok.io",
              "pathPrefix": "/"
            }
          ],
          "category": [
              "BROWSABLE",
              "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#F5F7FA",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#1A1D21"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ff00ff",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "567a8eb7-3de0-44ff-b899-e0f7b83d5118"
      }
    },
    "owner": "timothywashburn",
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "updates": {
      "url": "https://u.expo.dev/567a8eb7-3de0-44ff-b899-e0f7b83d5118",
      "checkAutomatically": "NEVER"
    }
  }
}
